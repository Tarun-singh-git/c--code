#include<iostream>
using namespace std;
int main(){
	int a[100],i,n,key,flag=0,count=0;
	cin>>n>>key;
	for(i=0;i<n;i++){
		cin>>a[i];
		if(a[i]==key){
			flag++;
			break;
		}
		else{
			count++;
		}
		}
		if(flag==0){
			cout<<"element not found"<<endl;
		}
		else{
			cout<<"element found in: "<<++count<<endl;
		}
	
}
